        <a href="/AcordosLeniencia"><div class="border0 sidebar-links menu-style">Controle Epidemiologico</div></a>
        <a href="/AcordosLeniencia"><div class="border0 sidebar-links menu-style">Controle Estoque</div></a>
    </div>
    <div class="content corpo">
        @RenderBody()
    </div>

    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
